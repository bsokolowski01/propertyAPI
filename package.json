{
  "name": "tapi_projekt",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "npx tsx ./tools/generator.ts",
    "startRestApi": "nodemon --watch . --ignore ./src/graphQL --ignore ./src/grpc --ext ts --exec npx tsx ./src/restAPI/index.ts",
    "startGraphQL": "nodemon --watch ./src/graphQL --ext ts --exec npx tsx ./src/graphQL/index.ts",
    "startGrpcServer": "nodemon --watch ./src/gRPC --ext ts --exec npx tsx ./src/gRPC/server.ts",
    "startGrpcClient": "npx tsx ./src/grpc/client.ts",
    "startDocs": "cd ./src/property-docs && npm install && npm start",
    "dev": "concurrently \"npm run startRestApi\" \"npm run startGraphQL\" \"npm run startGrpcServer\" "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@faker-js/faker": "^9.3.0",
    "@grpc/grpc-js": "^1.12.4",
    "@grpc/proto-loader": "^0.7.13",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "fs": "^0.0.1-security",
    "graphql": "^16.9.0",
    "npm": "^10.9.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-proto": "^2.6.0",
    "url": "^0.11.4",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "@types/cors": "^2.8.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/validator": "^13.12.2",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "5.4.2"
  }
}
